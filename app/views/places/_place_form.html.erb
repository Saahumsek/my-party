<div class="wrapper">
  <div class="container">
    <div class="row">
      <div class="col-xs-12 col-sm-6 col-sm-offset-3">
        <%= simple_form_for @place do |f| %>
          <%= f.input :name %>
          <%= f.input :address %>
          <%= f.input :capacity %>
          <%= f.input :price %>
          <%= f.button :submit %>
        <% end %>
      </div>
    </div>
  </div>
</div>
